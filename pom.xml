  <artifactId>jk-tool</artifactId>
  <packaging>jar</packaging>
  <version>0.0.2</version>

  <name>jk-tool</name>
  <description>jk-tool</description>
  <url>https://github.com/jukejuke/jk-tool</url>

  <licenses>
